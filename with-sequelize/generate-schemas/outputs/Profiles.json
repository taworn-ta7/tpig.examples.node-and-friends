{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://node-and-friends/with-sequelize/Profiles.json",
    "title": "Profile",
    "type": "object",
    "properties": {
        "id": {
            "$id": "https://node-and-friends/with-sequelize/properties/id",
            "type": "integer",
            "format": "int64"
        },
        "uid": {
            "$id": "https://node-and-friends/with-sequelize/properties/uid",
            "type": "integer",
            "format": "int64"
        },
        "name": {
            "$id": "https://node-and-friends/with-sequelize/properties/name",
            "type": "string",
            "maxLength": 100
        },
        "createdAt": {
            "$id": "https://node-and-friends/with-sequelize/properties/createdAt",
            "type": "string",
            "format": "date-time"
        },
        "updatedAt": {
            "$id": "https://node-and-friends/with-sequelize/properties/updatedAt",
            "type": "string",
            "format": "date-time"
        },
        "user": {
            "$ref": "#/definitions/Users"
        },
        "items": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/Items"
            }
        }
    },
    "required": [
        "id",
        "uid",
        "name",
        "createdAt",
        "updatedAt"
    ]
}