{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://node-and-friends/with-sequelize/Users.json",
    "title": "User",
    "type": "object",
    "properties": {
        "id": {
            "$id": "https://node-and-friends/with-sequelize/properties/id",
            "type": "integer",
            "format": "int64"
        },
        "username": {
            "$id": "https://node-and-friends/with-sequelize/properties/username",
            "type": "string",
            "maxLength": 20
        },
        "password": {
            "$id": "https://node-and-friends/with-sequelize/properties/password",
            "type": "string"
        },
        "createdAt": {
            "$id": "https://node-and-friends/with-sequelize/properties/createdAt",
            "type": "string",
            "format": "date-time"
        },
        "updatedAt": {
            "$id": "https://node-and-friends/with-sequelize/properties/updatedAt",
            "type": "string",
            "format": "date-time"
        },
        "profiles": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/Profiles"
            }
        }
    },
    "required": [
        "id",
        "username",
        "password",
        "createdAt",
        "updatedAt"
    ]
}